        img.npm(src="/images/icons/npm.png", alt="npm icon")
      .about-gallery
        img.mongo(src="/images/icons/mongo.png", alt="mongo db icon")
      .about-gallery
        img.pug(src="/images/icons/pug.svg", alt="pug/jade icon")
      .about-gallery
        img.wordpress(src="/images/icons/wordpress.png", alt="wordpress icon")
      .about-gallery
        img.shopify(src="/images/icons/shopify-logo.png", alt="Shopify Logo")
